<Window x:Class="NoDesk.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NoDesk"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="810">
    <Grid Margin="0,0,2,0">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="263*"/>
            <ColumnDefinition Width="137*"/>
        </Grid.ColumnDefinitions>
        <StackPanel Height="100" VerticalAlignment="Top" Orientation="Horizontal" Grid.ColumnSpan="2">
            <Button Content="Dashboard" MinWidth="200px" Click="Button_Click"/>
            <Button Content="indent managaement" MinWidth="200px"/>
        </StackPanel>
        <Frame x:Name="mainFrame" NavigationUIVisibility="Hidden" Content="Frame" HorizontalAlignment="Left" Height="450" Margin="0,119,0,0" VerticalAlignment="Top" Width="800" Grid.ColumnSpan="2"/>
    </Grid>
</Window>
